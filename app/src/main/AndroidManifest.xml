<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amruta.triviaapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_launcher_ic"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.HistoryListActivity"/>

        <activity android:name=".View.ResultActivity"
            android:parentActivityName=".View.QuizActivity"/>

        <activity android:name=".View.QuizActivity"
            android:parentActivityName=".View.MainActivity"/>

        <activity android:name=".View.MainActivity" />

        <activity android:name=".SplashActivity"><intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    </application>

</manifest>